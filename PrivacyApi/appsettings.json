{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "Data Source=database.db"
  },
  "Paths": {
    "DbInitSql": "Assets/database.sql",
    "Db": "database.db"
  },
  "JwtSettings": {
    "Secret": "YourSecretKeyHereMakeSureItIsAtLeast32BytesLong",
    "Issuer": "PrivacyApi",
    "Audience": "PrivacyExtension",
    "ExpirationDays": 7
  }
}
